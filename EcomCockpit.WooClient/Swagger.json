            required: false
            default: desc
            enum:
              - asc
              - desc
            description: Sortiert Attribute aufsteigend oder absteigend.
            type: string
          orderby:
            required: false
            default: date_gmt
            enum:
              - date
              - date_gmt
              - id
              - include
              - product
            description: Sortiere Liste nach Objektattribut.
