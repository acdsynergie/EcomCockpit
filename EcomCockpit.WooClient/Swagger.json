  '/wc/v3/products/(?P<product_id>[\d]+)/variations':
    namespace: wc/v3
    methods:
      - GET
      - POST
    endpoints:
      - methods:
          - GET
        args:
          product_id:
            required: false
            description: Eindeutige Kennung für das variable Produkt.
            type: integer
          context:
            required: false
            default: view
            enum:
              - view
              - edit
            description: >-
              Geltungsbereich der Anfrage; ermittelt in der Antwort vorhandene
              Felder.
            type: string
          page:
            required: false
            default: 1
            description: Aktuelle Seite der Sammlung.
            type: integer
          per_page:
            required: false
            default: 10
            description: >-
              Maximale Anzahl an Einträgen, die in einer Ergebnismenge
              ausgegeben werden. 
            type: integer
          search:
            required: false
            description: 'Zeigt nur die Ergebnisse an, die zu einer Zeichenkette passen. '
            type: string
          after:
            required: false
            description: >-
              Limitiert die Antwort auf Ressourcen welche nach einem bestimmten
              Datum (nach ISO8601-Standard) veröffentlicht wurden.
            type: string
          before:
            required: false
            description: >-
              Limitiert die Antwort auf Ressourcen welche vor einem bestimmten
              Datum (nach ISO8601-Standard) veröffentlicht wurden.
            type: string
          exclude:
            required: false
            default: []
            description: 'Sicherstellen, dass das Ergebnis bestimmte IDs ausschließt.'
            type: array
            items:
              type: integer
          include:
            required: false
            default: []
            description: Beschränkt Ergebnismenge auf bestimmte IDs.
            type: array
            items:
              type: integer
          offset:
            required: false
            description: Versieht die Ergebnismenge mit einem Offset.
            type: integer
          order:
            required: false
            default: desc
            enum:
              - asc
              - desc
            description: Sortiert Attribute aufsteigend oder absteigend.
            type: string
          orderby:
            required: false
            default: date
            enum:
              - date
              - id
              - include
              - title
              - slug
            description: Sortiere Liste nach Objektattribut.
            type: string
          parent:
            required: false
            default: []
            description: 'Beschränkt Ergebnismenge auf bestimmte Eltern-IDs. '
            type: array
            items:
              type: integer
          parent_exclude:
            required: false
            default: []
            description: >-
              Beschränkt Ergebnismenge auf alle Elemente, außer denen, die einer
              bestimmten Eltern-ID zugeordnet werden.
            type: array
            items:
              type: integer
          slug:
            required: false
            description: Beschränkt Ergebnismenge auf Produkte mit einem bestimmten Slug.
            type: string
          status:
            required: false
            default: any
            enum:
              - any
              - future
              - draft
              - pending
              - private
              - publish
            description: >-
              Beschränkt Ergebnismenge auf Produkte, denen einen bestimmter
              Status zugeordnet wird.
            type: string
          sku:
            required: false
            description: >-
              Beschränkt Ergebnismenge auf Produkte mit bestimmten
              Artikelnummern. Verwende Kommas zum Trennen.
            type: string
          tax_class:
            required: false
            enum:
              - standard
              - 0-preis
              - reduzierter-preis
              - virtual-rate
              - virtual-reduced-rate
            description: >-
              Ergebnis auf Produkte mit einer bestimmten Steuerklasse
              einschränken.
            type: string
          on_sale:
            required: false
            description: Beschränkt Ergebnismenge auf Produkte im Angebot.
            type: boolean
          min_price:
            required: false
            description: >-
              Beschränkt Ergebnismenge auf Produkte basierend auf dem
              Minimumpreis.
            type: string
          max_price:
            required: false
            description: >-
