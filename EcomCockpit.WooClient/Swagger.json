              Sicherstellen, dass in der Ergebnismenge Bewertungen
              ausgeschlossen sind, die bestimmten Benutzer-IDs zugewiesen sind.
            type: array
            items:
              type: integer
          reviewer_email:
            required: false
            description: >-
              Ergebnismenge auf eine bestimmte Autoren-E-Mail-Adresse
              beschränken.
            type: string
          product:
            required: false
            default: []
            description: >-
              Das Ergebnis auf Bewertungen beschränken, die bestimmten
              Produkt-IDs zugewiesen sind.
            type: array
            items:
              type: integer
          status:
            required: false
            default: approved
            enum:
              - all
              - hold
              - approved
              - spam
              - trash
            description: >-
              Ergebnismenge auf Bewertungen beschränken, denen ein bestimmter
              Status zugewiesen ist.
            type: string
      - methods:
          - POST
        args:
          product_id:
            required: true
            description: Eindeutige Kennung für das Produkt.
            type: integer
          status:
            required: false
            default: approved
            enum:
              - approved
              - hold
              - spam
              - unspam
              - trash
              - untrash
            description: Status der Bewertung.
            type: string
          reviewer:
            required: true
            description: Name des Rezensenten.
            type: string
          reviewer_email:
            required: true
            description: E-Mail des Rezensenten.
            type: string
          review:
            required: true
            description: Inhalt bewerten.
            type: string
          rating:
            required: false
            description: Bewertung (von 0 bis 5).
