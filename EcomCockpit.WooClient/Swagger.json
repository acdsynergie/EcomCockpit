            required: false
            description: Eindeutige Kennung für das Attribut der Begriffe.
            type: integer
          context:
            required: false
            default: view
            enum:
              - view
              - edit
            description: >-
              Geltungsbereich der Anfrage; ermittelt in der Antwort vorhandene
              Felder.
            type: string
          page:
            required: false
            default: 1
            description: Aktuelle Seite der Sammlung.
            type: integer
          per_page:
            required: false
            default: 10
            description: >-
              Maximale Anzahl an Elementen, die im Ergebnissatz zurückgegeben
              werden.
            type: integer
          search:
            required: false
            description: >-
              Ergebnisse auf alle begrenzen, die mit einer Zeichenkette
              übereinstimmen.
            type: string
          exclude:
            required: false
            default: []
            description: 'Sicherstellen, dass das Ergebnis bestimmte IDs ausschließt.'
            type: array
            items:
              type: integer
          include:
            required: false
            default: []
            description: Beschränkt Ergebnismenge auf bestimmte IDs.
            type: array
            items:
              type: integer
          order:
            required: false
            default: asc
            enum:
              - asc
