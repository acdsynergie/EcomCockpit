name: wp-dev sandbox
description: developer playground
url: 'http://localhost:81/wordpress'
home: 'http://localhost:81/wordpress'
gmt_offset: '1'
timezone_string: ''
namespaces:
  - oembed/1.0
  - akismet/v1
  - jetpack/v4
  - wpcom/v2
  - jwt-auth/v1
  - wc-analytics
  - wc-admin
  - wc/blocks
