      - methods:
          - POST
        args:
          code:
            required: true
            description: Gutscheincode.
            type: string
          amount:
            required: false
            description: >-
              Die Höhe des Rabatts (Rabattbetrag). Sollte immer numerisch sein,
              auch wenn es ein Prozentsatz ist.
            type: string
          discount_type:
            required: false
            default: fixed_cart
            enum:
              - percent
              - fixed_cart
              - fixed_product
            description: 'Bestimmt die Rabattart, die angewendet wird.'
            type: string
          description:
            required: false
            description: Beschreibung des Gutscheins.
            type: string
          date_expires:
            required: false
            description: >-
              Das Datum, an dem der Gutschein abläuft, in der Zeitzone der
              Website.
            type: string
          date_expires_gmt:
            required: false
            description: 'Das Datum, an dem der Gutschein abläuft, als GMT.'
            type: string
          individual_use:
            required: false
            default: false
            description: >-
              Wenn aktiv kann dieser Gutschein nur einzeln benutzt werden.
              Andere angewendete Gutscheine werden vom Warenkorb entfernt.
            type: boolean
          product_ids:
            required: false
            description: 'Liste der Produkt-IDs, für die der Gutschein benutzt werden kann.'
            type: array
            items:
              type: integer
          excluded_product_ids:
            required: false
            description: >-
              Liste der Produkt-IDs, auf die der Gutschein nicht angewendet
              werden kann.
            type: array
            items:
              type: integer
          usage_limit:
            required: false
            description: Wie oft der Gutschein insgesamt benutzt werden kann.
            type: integer
          usage_limit_per_user:
            required: false
            description: Wie oft der Gutschein pro Kunde verwendet werden kann.
            type: integer
          limit_usage_to_x_items:
            required: false
            description: >-
              Maximale Anzahl der Artikel auf die der Gutschein angewendet
              werden kann.
            type: integer
          free_shipping:
            required: false
            default: false
            description: >-
              Wenn aktiv und die kostenlose Versandmethode einen Gutschein
              benötigt, wird dieser Gutschein kostenlosen Versand aktivieren.
            type: boolean
          product_categories:
            required: false
            description: 'Liste von Kategorie-IDs, auf die der Gutschein zutrifft.'
            type: array
            items:
              type: integer
          excluded_product_categories:
            required: false
            description: 'Liste von Kategorie-IDs, auf die der Gutschein nicht zutrifft.'
            type: array
            items:
              type: integer
          exclude_sale_items:
            required: false
            default: false
            description: >-
              Wenn dies der Fall ist, wird dieser Gutschein nicht auf
